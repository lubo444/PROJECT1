<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Test\Bundle\CompanyBundle\Entity\OpeningHours" table="opening_hours">
    <indexes>
      <index name="fk__opnng_hrs__office__id_office" columns="id_office"/>
    </indexes>
    <id name="idOpnngHrs" type="integer" column="id_opnng_hrs">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dayInWeek" type="integer" column="day_in_week" nullable="false"/>
    <field name="startAt" type="string" column="start_at" length="15" nullable="true"/>
    <field name="endAt" type="string" column="end_at" length="15" nullable="true"/>
    <field name="lunchStartAt" type="string" column="lunch_start_at" length="15" nullable="true"/>
    <field name="lunchEndAt" type="string" column="lunch_end_at" length="15" nullable="true"/>
    <many-to-one field="idOffice" target-entity="Office">
      <join-columns>
        <join-column name="id_office" referenced-column-name="id_office"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
